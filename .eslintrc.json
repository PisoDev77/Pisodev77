{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": [
    "eslint:recommended",
    "plugin:react/recommended",
    "plugin:react/jsx-runtime",
    "airbnb",
    "prettier"
  ],
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "plugins": ["react", "import", "html", "react-hooks"],
  "rules": {
    "vars-on-top": "off",
    "import/no-unresolved": "off",
    "no-shadow": "off",
    "import/extensions": "off",
    "react/react-in-jsx-scope": "off"
  }
}
