{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": [
    "plugin:react/recommended",
    "plugin:react/jsx-runtime",
    "airbnb",
    "prettier"
  ],
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": ["react", "import", "html"],
  "rules": {
    "vars-on-top": "off",
    "import/no-unresolved": "off",
    "no-shadow": "off",
    "import/extensions": "off"
  }
}
